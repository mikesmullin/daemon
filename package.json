{
  "name": "daemon",
  "version": "2.0.0",
  "type": "module",
  "description": "File-based multi-agent orchestrator using GitHub Copilot API",
  "main": "src/daemon.mjs",
  "bin": {
    "d": "./src/daemon.mjs"
  },
  "scripts": {
    "help": "bun src/daemon.mjs help",
    "clean": "bun src/daemon.mjs clean",
    "postinstall": "bash scripts/install-plugins.sh",
    "test": "./run-tests.sh",
    "test:watch": "bun test --watch tests/",
    "test:coverage": "bun test --coverage",
    "test:ui": "bun run test:e2e && bun run test:unit",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:unit": "web-test-runner",
    "test:unit:watch": "web-test-runner --watch",
    "test:integration": "bun test tests/integration/",
    "test:all": "bun run test && bun run test:ui"
  },
  "keywords": [
    "github",
    "copilot",
    "cli",
    "ai",
    "multi-agent",
    "orchestrator",
    "file-based"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@google/genai": "^0.4.0",
    "@google/generative-ai": "^0.21.0",
    "bun-pty": "^0.3.2",
    "chokidar": "^3.6.0",
    "clipboardy": "^5.0.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "ollama": "^0.5.11",
    "open": "^10.2.0",
    "openai": "^6.1.0"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/testing": "^4.0.0",
    "@open-wc/testing-helpers": "^3.0.0",
    "@playwright/test": "^1.56.1",
    "@web/test-runner": "^0.20.2",
    "@web/test-runner-playwright": "^0.11.1",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.56.1"
  }
}
